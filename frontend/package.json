{
  "name": "celebra-capital-frontend",
  "private": true,
  "version": "0.2.3",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "jest",
    "test:unit": "jest --selectProjects unit",
    "test:integration": "jest --selectProjects integration",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "check-coverage": "node scripts/check-coverage.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:a11y": "node scripts/run-a11y-tests.js",
    "test:a11y:watch": "nodemon --watch src --ext js,jsx,ts,tsx --exec 'npm run test:a11y'",
    "test:a11y:report": "node scripts/run-a11y-tests.js && open a11y-reports/a11y-report-*.html",
    "test:a11y:ci": "node scripts/run-a11y-tests.js --ci",
    "audit:a11y": "node scripts/a11y-audit.js",
    "audit:a11y:mock": "cross-env MOCK_RESULTS=true node scripts/a11y-audit.js",
    "a11y:report": "start a11y-reports/a11y-report-latest.html",
    "test:a11y:components": "node scripts/test-component-a11y.js",
    "test:a11y:button": "node scripts/test-button-a11y.js",
    "start": "react-scripts start",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@headlessui/react": "^2.2.2",
    "@hookform/resolvers": "^3.3.2",
    "@mui/icons-material": "^5.17.1",
    "@mui/material": "^5.17.1",
    "@sentry/react": "^7.80.1",
    "@sentry/tracing": "^7.120.3",
    "@tanstack/react-query": "^5.74.4",
    "@tanstack/react-query-devtools": "^5.74.6",
    "axios": "^1.6.2",
    "chart.js": "^4.4.9",
    "framer-motion": "^12.9.4",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^5.0.0",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.48.2",
    "react-router-dom": "^6.19.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4",
    "zod": "^3.22.4",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@axe-core/puppeteer": "^4.8.2",
    "@axe-core/react": "^4.7.3",
    "@eslint/js": "^9.25.1",
    "@storybook/addon-a11y": "^7.6.10",
    "@storybook/addon-essentials": "^7.6.10",
    "@storybook/addon-interactions": "^7.6.10",
    "@storybook/addon-links": "^7.6.10",
    "@storybook/addon-onboarding": "^1.0.11",
    "@storybook/blocks": "^7.6.10",
    "@storybook/react": "^7.6.10",
    "@storybook/react-vite": "^7.6.10",
    "@storybook/test": "^7.6.10",
    "@tailwindcss/forms": "^0.5.10",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^16.18.61",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-helmet": "^6.1.11",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "cypress": "^13.6.4",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "globals": "^16.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-axe": "^8.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-image-snapshot": "^6.4.0",
    "msw": "^1.3.2",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.32",
    "puppeteer": "^21.5.0",
    "storybook": "^7.6.10",
    "tailwindcss": "^3.3.6",
    "terser": "^5.29.1",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5",
    "typescript-eslint": "^8.31.0",
    "vite": "^5.0.8",
    "vite-imagetools": "^6.2.8",
    "vite-plugin-pwa": "^0.17.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
